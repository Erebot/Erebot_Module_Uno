#
msgid   "player.move.illegal"
msgstr  "<b><var name=\"nick\"/></b>'s move <b>WAS NOT</b> legal. "
        "<b><var name=\"nick\"/></b> must pick <b><var name=\"count\"/></b> "
        "cards!"
#
msgid   "player.joins"
msgstr  "<b><var name=\"nick\"/></b> joins this <var name=\"logo\"/> game."
#
msgid   "player.draws.penalty"
msgstr  "<b><var name=\"nick\"/></b> passes turn, and has to pick "
        "<b><var name=\"count\"/></b> cards!"
#
msgid   "player.draws"
msgstr  "<b><var name=\"nick\"/></b> draws a card"
#
msgid   "player.drawn"
msgstr  "You drew: <for from=\"cards\" item=\"card\" separator=\" "
        "\"><var name=\"card\"/></for>"
#
msgid   "player.deals"
