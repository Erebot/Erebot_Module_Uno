        "\" separator=\" \"><var name=\"card\"/></for>"
#
msgid   "player.please-choose"
msgstr  "<var name=\"logo\"/> <b><var name=\"nick\"/></b>, please "
        "choose a color with <b><var name=\"cmd\"/> &lt;r|b|g|y&gt;</b>"
#
msgid   "player.plays"
msgstr  "<b><var name=\"nick\"/></b> plays <var name=\"card\"/> "
        "<b><var name=\"count\"/> times!</b>"
#
msgid   "player.penalty"
msgstr  "<var name=\"logo\"/> <b><var name=\"nick\"/></b> must draw "
        "<b><var name=\"count\"/></b> cards."
#
msgid   "player.passes"
msgstr  "<b><var name=\"nick\"/></b> passes turn"
#
msgid   "player.move.legal"
